    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^10.0.0",
    "cookie": "^0.7.2",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
