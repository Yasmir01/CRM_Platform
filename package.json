    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/system": "^7.1.0",
    "@mui/x-charts": "^8.3.1",
    "@mui/x-data-grid": "^8.9.1",
    "@mui/x-date-pickers": "^8.10.2",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "^6.15.0",
    "@stripe/react-stripe-js": "^4.0.0",
