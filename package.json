{
  "name": "crm-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "if [ -n \"$DATABASE_URL\" ]; then prisma db push --schema prisma/schema.prisma; else echo \"Skipping Prisma db push: DATABASE_URL not set\"; fi && prisma generate --schema prisma/schema.prisma && vite build",
    "preview": "vite preview",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:push": "prisma db push --schema prisma/schema.prisma",
    "prisma:seed": "node prisma/seed.mjs",
    "prisma:seed-demo": "node prisma/seed-demo.mjs",
    "prisma:migrate": "prisma migrate dev --name sync-relations"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "6.4.0",
    "@mui/lab": "6.0.0-beta.22",
    "@mui/material": "6.4.0",
    "@mui/x-charts": "8.3.1",
    "@mui/x-data-grid": "8.9.1",
    "@mui/x-date-pickers": "8.10.2",
    "@paypal/paypal-server-sdk": "1.0.3",
    "@prisma/client": "6.15.0",
    "@stripe/react-stripe-js": "4.0.0",
    "@stripe/stripe-js": "7.9.0",
    "@tanstack/react-query": "5.62.7",
    "axios": "1.7.9",
    "bcryptjs": "2.4.3",
    "chart.js": "4.4.6",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "express": "5.0.1",
    "framer-motion": "11.15.0",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "0.475.0",
    "nodemailer": "6.9.16",
    "react": "19.0.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-router-dom": "7.0.2",
    "tailwindcss": "3.4.15",
    "use-sync-external-store": "1.5.0",
    "uuid": "11.1.0",
    "xlsx": "0.18.5",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "autoprefixer": "10.4.20",
    "postcss": "8.4.49",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.7.2",
    "vite": "6.3.5",
    "ts-node": "10.9.1",
    "@types/node": "20.5.1"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
